{
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "imports": {
    "react": "npm:react@^18.2.0",
    "react-dom": "npm:react-dom@^18.2.0",
    "react/": "npm:react/",
    "react-dom/": "npm:react-dom/",
    "msw": "npm:msw@^2.0.0"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    },
    "exclude": ["dist/", "build/"]
  },
  "fmt": {
    "exclude": ["dist/", "build/"]
  }
}